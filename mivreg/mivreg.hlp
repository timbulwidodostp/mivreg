{synopt:{cmd:e(rmse)}}root mean squared error{p_end}
{synopt:{cmd:e(F)}}model F statistic{p_end}
{synopt:{cmd:e(F1)}}first-stage F statistic{p_end}
{synopt:{cmd:e(df_m_F1)}}first-stage model degrees of freedom{p_end}
{synopt:{cmd:e(df_r_F1)}}first-stage residual degrees of freedom{p_end}
{synopt:{cmd:e(df_m)}}model degrees of freedom{p_end}
{synopt:{cmd:e(df_r)}}residual degrees of freedom{p_end}
{synopt:{cmd:e(r2)}}R^2{p_end}
{synopt:{cmd:e(r2_a)}}adjusted R^2{p_end}
{synopt:{cmd:e(r2_1)}}first-stage R^2{p_end}
{synopt:{cmd:e(jval)}}model J statistic{p_end}
{synopt:{cmd:e(jpv)}}J-test p-value{p_end}

