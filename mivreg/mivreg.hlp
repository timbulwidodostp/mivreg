{synopt:{cmd:e(title)}}title in estimation output{p_end}
{synopt:{cmd:e(properties)}}{cmd:b} {cmd:V}{p_end}
{synopt:{cmd:e(model)}}{cmd:hom} or {cmd:het}{p_end}
